<f:layout name="Default" />

<f:section name="Title">Edit ranking list "{rankingList.name}"</f:section>

<f:section name="Content">
	<f:form action="update" object="{rankingList}" objectName="rankingList">
		<ol>
			<li>
				<label for="overview">Overview</label>
				<f:form.textfield property="overview" id="overview" />
			</li>
		
			<li>
				<label for="supervisoryBoard">Supervisory board</label>
				<f:form.textfield property="supervisoryBoard" id="supervisoryBoard" />
			</li>
		
			<li>
				<label for="name">Name</label>
				<f:form.textfield property="name" id="name" />
			</li>
		
			<li>
				<label for="description">Description</label>
				<f:form.textfield property="description" id="description" />
			</li>
		
			<li>
				<f:form.submit value="Update" />
			</li>
		</ol>
	</f:form>
</f:section>